{
  "name": "eslint-config-bambi",
  "version": "1.0.1",
  "description":
    "Shared ESLint configs for Node, Web, React Native, and Expo projects",
  "main": "default.js",
  "scripts": {
    "lint": "eslint .",
    "test": "jest"
  },
  "keywords": ["eslint-config", "expo", "bam", "react-native"],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bam/bambi.git"
