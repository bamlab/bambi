  "bugs": {
    "url": "https://github.com/bam/bambi/issues"
  },
  "homepage": "https://github.com/bam/bambi#readme",
  "dependencies": {
    "babel-eslint": "^7.2.3",
    "eslint-config-prettier": "^2.7.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-config-promise": "^1.2.1",
    "eslint-plugin-react": "^7.4.0"
  },
  "devDependencies": {
    "@expo/spawn-async": "^1.3.0",
    "eslint": "^4.11.0",
    "jest": "^21.2.1",
    "prettier": "^1.8.2"
  },
  "peerDependencies": {
    "eslint": "^4.11.0",
    "prettier": "^1.8.2"
